<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/vehiclename_item_parent"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/btn_none_white">



    <TextView
        android:id="@+id/vehiclename_item_icon"
        android:layout_width="@dimen/_20sdp"
        android:layout_height="@dimen/_20sdp"
        android:fontFamily="@font/font_awesome"
        android:background="@drawable/bg_oval_lightgrey"
        android:layout_marginLeft="@dimen/_20sdp"
        android:layout_marginTop="@dimen/_10sdp"
        android:layout_marginBottom="@dimen/_10sdp"
        android:text="@string/icon_car"
        android:textColor="@color/black_text_color_hint"
        android:layout_centerVertical="true"
        android:gravity="center"/>

    <TextView
        android:id="@+id/vehiclename_item_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Tran van A"
        android:textColor="@color/black_text_color"
        android:textSize="@dimen/_12sdp"
        android:layout_toRightOf="@+id/vehiclename_item_icon"
        android:layout_centerInParent="true"
        android:layout_marginLeft="@dimen/_10sdp"/>

    <TextView
        android:id="@+id/vehiclename_item_kind"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tay"
        android:textColor="@color/colorBlueTransparent"
        android:textSize="@dimen/_10sdp"
        android:layout_centerInParent="true"
        android:layout_marginRight="@dimen/_10sdp"
        android:layout_alignParentRight="true"/>

    <View
        android:id="@+id/seperateline"
        android:layout_width="match_parent"
        android:layout_height="1dip"
        android:background="#0d000000"
        android:layout_marginLeft="@dimen/_40sdp"
        android:layout_alignParentBottom="true"/>

</RelativeLayout>