<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/view_list_admin_parent"
    android:layout_width="match_parent"
    android:layout_height="@dimen/_40sdp"
    tools:ignore="WebViewLayout"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:fontFamily="@font/font_awesome"
            android:id="@+id/list_admin_icon"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:minWidth="@dimen/_40sdp"
            android:textColor="@color/yellow_pressed"
            android:textSize="@dimen/_12sdp"
            android:text="@string/icon_key"
            android:gravity="center" />



        <TextView
            android:id="@+id/list_admin_text"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="Tân Bình"
            android:textColor="@color/black_text_color"
            android:textSize="@dimen/_12sdp"
            android:layout_gravity="center_vertical"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/_5sdp"
            android:textStyle="bold"/>

        <TextView
            android:id="@+id/list_admin_phone"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="Tân Bình"
            android:textColor="@color/black_text_color"
            android:textSize="@dimen/_10sdp"
            android:layout_gravity="center_vertical"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/_10sdp" />



    </LinearLayout>

    <ProgressBar
        android:id="@+id/list_admin_progress"
        android:layout_width="@dimen/_15sdp"
        android:layout_height="@dimen/_15sdp"
        android:layout_toLeftOf="@+id/list_admin_active"
        android:layout_centerInParent="true"
        android:layout_marginLeft="@dimen/_5sdp"
        android:indeterminateTint="@color/colorBlue"
        android:visibility="gone"/>

    <com.suke.widget.SwitchButton
        android:id="@+id/list_admin_active"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/_20sdp"
        app:sb_checked="true"
        app:sb_checked_color="@color/black_text_color_hint"
        android:layout_marginRight="@dimen/_10sdp"
        android:layout_alignParentRight="true"
        android:layout_centerInParent="true"/>

    <TextView
        android:id="@+id/list_admin_cover"
        android:layout_width="@dimen/_40sdp"
        android:layout_height="match_parent"
        android:layout_marginRight="@dimen/_10sdp"
        android:layout_alignParentRight="true"/>

    <View
        android:id="@+id/list_admin_line"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#e0e0e0"
        android:layout_marginLeft="@dimen/_10sdp"
        android:layout_marginRight="@dimen/_10sdp"
        android:layout_alignParentBottom="true" />

</RelativeLayout>

